CREATE TABLE T_REQUEST
(
	F_NO       INTEGER NOT NULL,
	F_YEAR     INTEGER NOT NULL,
	F_TYPE     CHAR(1) NOT NULL,

	F_KIND     CHAR(1) NOT NULL,
	F_COLLEGE  CHAR(1) NOT NULL,

	F_TEXT     CHAR(L_ORIGIN_TEXT),
	F_DATE     DATE NOT NULL,

	/* 2007:149 LPR: removed all sender fields */

	F_SUBJECT      INTEGER,
	F_QUICK        INTEGER,
	F_TIME         TIME,
	F_DIRECTION    CHAR(L_DIRECTION),  /* 2010:048 */
	F_EISPP_NMR    CHAR(L_EISPP_NMR),  /* 2012:289 */
	F_OLD_KIND     CHAR(1),            /* 2013:213 */
	F_OLD_SUBJECT  INTEGER,            /* 2016:056 */
	F_LAW_KIND     CHAR(1),            /* 2018:086 */

	CONSTRAINT I_REQUEST PRIMARY KEY (F_NO, F_YEAR, F_TYPE),
	CONSTRAINT C_REQUEST_NO CHECK D_XFER_NO(F_NO),
	CONSTRAINT C_REQUEST_YEAR CHECK D_YEAR(F_YEAR),
	CONSTRAINT C_REQUEST_TYPE CHECK D_REQUEST_TYPE(F_TYPE),
	CONSTRAINT C_REQUEST_KIND CHECK D_KIND(F_KIND),
	CONSTRAINT C_REQUEST_COLLEGE CHECK D_COLLEGE(F_COLLEGE),
	CONSTRAINT C_REQUEST_SUBJECT CHECK D_OPT_SUBJECT(F_SUBJECT),
	CONSTRAINT C_REQUEST_OLD_KIND CHECK D_OPT_KIND(F_OLD_KIND),
	CONSTRAINT C_REQUEST_LAW_KIND CHECK D_OPT_KIND(F_KIND)
);

