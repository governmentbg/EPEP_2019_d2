	CONSTRAINT C_LAWSUIT_SOURCE_YEAR CHECK D_OPT_YEAR(F_SOURCE_YEAR),
	CONSTRAINT C_LAWSUIT_SOURCE_TYPE CHECK D_OPT_TYPE(F_SOURCE_TYPE),
	CONSTRAINT C_LAWSUIT_SOURCE_KIND CHECK D_KIND(F_SOURCE_KIND),
